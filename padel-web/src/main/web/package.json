{
    "name": "padel",
    "version": "1.0.0",
    "private": true,
    "description": "Padel planning app",
    "scripts": {
        "lint": "tslint ./src/**/*.ts -t verbose",
        "start": "ng serve",
        "package": "npm install && npm run build",
        "test": "ng test",
        "build": "ng build --prod --base-href=\"/padel/\"",
        "e2e": "ng e2e",
        "ng": "ng",
        "upgrade": "ng update @angular/core@9 @angular/cli@9 --allow-dirty",
        "upgrade-9": "ng update @angular/core @angular/cli --allow-dirty --force",
        "upgrade-material": "ng update @angular/material --allow-dirty --force",
        "upgrade-store": "ng update @ngrx/store --allow-dirty --force",
        "upgrade-grid": "ng update ag-grid-angular ag-grid-community --allow-dirty --force",
        "localize": "ng add @angular/localize"
    },
